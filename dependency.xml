<?xml version="1.0" encoding="UTF-8"?>
<project name="MapCircuitBoard" default="builddep">
	<!-- Environments, do not modify! -->
	<property name="dir.src" value="src"/>
	<property name="dir.bin" value="bin"/>
	<property name="dir.j7bin" value="j7bin"/>
	<property name="dir.dep" value="dep"/>
	
	<!-- Modify when API changes -->
	<property file="${basedir}/build.properties"/>
	
	<!-- Do not modify the following code -->
	<property name="artifact" value="mapcircuitboard"/>
	<property name="classpath" value="${dir.bin}:${dir.dep}/mappainting.jar:${dir.dep}/bukkit.jar:${dir.dep}/googlecollect.jar:${dir.dep}/ebeanorm.jar"/>
	
	<target name="builddep">
		<mkdir dir="${dir.dep}"/>
		<get src="${mappainting}" dest="${dir.dep}/mappainting.jar" verbose="true"/>
		<get src="${bukkit}" dest="${dir.dep}/bukkit.jar" verbose="true"/>
		<get src="${retrolambda}" dest="${dir.dep}/retrolambda.jar" verbose="true"/>
		<get src="${googlecollect}" dest="${dir.dep}/googlecollect.jar" verbose="true"/>
		<get src="${ebeanorm}" dest="${dir.dep}/ebeanorm.jar" verbose="true"/>
	</target>
</project>