<?xml version="1.0" encoding="UTF-8"?>
<project name="MapCircuitBoard" default="builddep">
	<!-- Environments, do not modify! -->
	<property name="dir.src" value="src"/>
	<property name="dir.bin" value="bin"/>
	<property name="dir.j7bin" value="j7bin"/>
	<property name="dir.dep" value="dep"/>
	<property name="ps" value="${path.separator}"/>
	<property name="fs" value="${file.separator}"/>
	
	<!-- Modify when API changes -->
	<property file="${basedir}${fs}build.properties"/>
	
	<!-- Do not modify the following code -->
	<property name="artifact" value="mapcircuitboard"/>
	
	<target name="builddep">
		<mkdir dir="${dir.dep}"/>
		<get src="${mappainting}" dest="${dir.dep}${fs}mappainting.jar" verbose="true"/>
		<get src="${bukkit}" dest="${dir.dep}${fs}bukkit.jar" verbose="true"/>
		<get src="${retrolambda}" dest="${dir.dep}${fs}retrolambda.jar" verbose="true"/>
		<get src="${googlecollect}" dest="${dir.dep}${fs}googlecollect.jar" verbose="true"/>
		<get src="${ebeanorm}" dest="${dir.dep}${fs}ebeanorm.jar" verbose="true"/>
	</target>
</project>